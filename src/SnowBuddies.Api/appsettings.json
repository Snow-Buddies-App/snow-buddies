{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/snowbuddieslog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "SBConnectionString": "Server=localhost;Database=SnowBuddiesDb;Username=postgres"
    }
  }
